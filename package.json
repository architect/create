{
  "name": "@architect/create",
  "version": "1.3.0-RC.3",
  "description": "Idempotently initialize Architect projects",
  "main": "src/index.js",
  "bin": {
    "arc-create": "cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/architect/create.git"
  },
  "scripts": {
    "test": "npm run lint && npm run coverage",
    "test:unit": "cross-env PORT=6666 tape 'test/unit/**/*-test.js' | tap-spec",
    "coverage": "nyc --reporter=lcov --reporter=text-summary npm run test:unit",
    "lint": "eslint . --fix",
    "rc": "npm version prerelease --preid RC"
  },
  "engines": {
    "node": ">=10"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@architect/inventory": "~1.1.0-RC.10",
    "@architect/utils": "~1.5.14",
    "chalk": "~4.1.0",
    "run-parallel": "~1.1.10"
  },
  "devDependencies": {
    "@architect/eslint-config": "1.0.0",
    "cross-env": "~7.0.2",
    "eslint": "^7.13.0",
    "nyc": "~15.1.0",
    "proxyquire": "^2.1.3",
    "tap-spec": "^5.0.0",
    "tape": "^5.0.1"
  },
  "eslintConfig": {
    "extends": "@architect/eslint-config"
  }
}
